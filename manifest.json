{
    "name": "YouTube Ad Skip",
    "version": "1.0.0.0",
    "manifest_version": 2,
    "description": "a extention for Google Chrome",
    "content_scripts": [{
        "matches": [ "http://*/*", "https://*/*" ],
        "js": ["youtube_ad_skip.js"]
    }],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": ["activeTab"]
  }